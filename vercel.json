{
  "builds": [
    {
      "src": "migrate.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "templates"
      }
    },
    {
        "src": "*.py",
        "use": "@vercel/python",
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "core/wsgi.py"
    }
  ]
}
